type User {
    id: Int!
    username: String!
    email: String!
    password: String!
    created_at: Int!
    updated_at: Int!
}

input NewUserInput {
    username: String!
    email: String!
    password: String!
}

type Query {
    users: [User]!
}

type Mutation {
    createUser(input: NewUserInput!): User!
}
